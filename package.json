{
  "name": "fbo-lambda-template",
  "version": "1.0.0",
  "description": "Private Lambda function template for FBO operations - Yummy Inc",
  "main": "index.mjs",
  "type": "module",
  "homepage": "https://github.com/yummysuperapp/fbo-lambda-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yummysuperapp/fbo-lambda-template.git"
  },
  "bugs": {
    "url": "https://github.com/yummysuperapp/fbo-lambda-template/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "package": "npm run build",
    "start": "node index.mjs",
    "start:dev": "tsx src/index.ts",
    "start:local": "NODE_ENV=local tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "deploy": "node scripts/deploy.mjs",
    "deploy:dev": "npm run deploy dev",
    "deploy:testing": "npm run deploy testing",
    "deploy:prod": "npm run deploy prod",
    "check-all": "npm run type-check && npm run lint && npm run test",
    "prepare-deploy": "npm run check-all && npm run build"
  },
  "keywords": [
    "fbo",
    "financial-backoffice",
