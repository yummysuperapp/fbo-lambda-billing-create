{
  "name": "fbo-lambda-template",
  "version": "1.0.0",
  "description": "Private Lambda function template for FBO operations - Yummy Inc",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node index.mjs",
    "start:dev": "tsx src/index.ts",
    "start:local": "NODE_ENV=local tsx src/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "package": "npm run build && zip -r lambda-deployment.zip dist/ node_modules/ package.json index.mjs",
    "deploy": "node scripts/deploy.mjs",
    "deploy:dev": "npm run deploy dev",
    "deploy:prod": "npm run deploy prod"
  },
  "keywords": [
    "fbo",
    "finance",
    "lambda",
    "yummy",
    "typescript",
    "aws",
    "postgresql",
    "mongodb"
  ],
  "author": "Yummy Inc",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.738.0",
    "@google-cloud/bigquery": "^8.1.1",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "mongodb": "^6.10.0",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": "22.x"
  },
  "private": true
}
